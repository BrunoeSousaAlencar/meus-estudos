algoritmo "contadornovo"
var
    contador, n: inteiro
inicio
       escreva("Eu devo contar ate qual número?: ")
          leia (n)
       contador <- 0
       enquanto (contador <= n) faca
          escreval (contador)
       contador <-  contador + 1
       fimenquanto
       escreval("terminei de contar até ",n)
fimalgoritmo