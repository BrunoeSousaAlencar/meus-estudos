algoritmo "semnome"
var
      r: vetor[1..6] de inteiro
      c: inteiro
inicio
        escreval("PASSO 1 - Cadastro de Gabarito")
        escreval("------------------------------")
        c <- 1
        enquanto c < 6 faca
           escreva("Questao ",,": ")
                leia(c)
           c <- c + 1
           
        
        fimenquanto


        
fimalgoritmo